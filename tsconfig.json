{
    "compilerOptions": {
    "module": "commonjs",
    "target": "es2020",
    "lib": [
        "es2015",
        "es2018",
        "es2020"
    ],
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "./dist",
        "rootDir": "./src",
        "paths": {
            "@app/*": [
            "./src/*"
            ]
            },
    },
"exclude": [
    "node_modules",
    "dist"
]
}